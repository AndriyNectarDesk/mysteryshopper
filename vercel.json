{
  "name": "ottawa-mystery-shoppers",
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/success",
      "dest": "/success.html"
    },
    {
      "src": "/thanks",
      "dest": "/thanks.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 